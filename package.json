{
  "name": "practice-modern-javascript",
  "version": "3.2.1",
  "description": "A boilerplate to setup and practice Modern JavaScript",
  "main": "build/index.js",
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "better-console": "^1.0.0",
    "cfonts": "^1.1.3",
    "clor": "^5.2.0",
    "debug": "^2.6.8",
    "decimal.js": "^7.2.2",
    "dedent": "^0.7.0",
    "lodash": "^4.12.0",
    "npm-run-all": "^4.0.2",
    "numd": "^3.0.2",
    "ora": "^1.3.0",
    "ordinal-number-suffix": "^0.1.1",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-comments": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-merge-sibling-variables": "^6.8.3",
    "babel-plugin-transform-minify-booleans": "^6.8.0",
    "babel-plugin-transform-property-literals": "^6.8.0",
    "babel-plugin-transform-remove-debugger": "^6.8.0",
    "babel-plugin-transform-simplify-comparison-operators": "^6.8.0",
    "babel-plugin-transform-undefined-to-void": "^6.8.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "^2.4.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "flow-bin": "^0.48.0"
  },
  "scripts": {
    "prepublish": "run-s build",
    "start": "node .",
    "build": "run-s clean:build build:babel:prod",
    "watch": "run-s watch:dev",
    "watch:dev": "nodemon -q -w build build",
    "watch:build": "run-s clean:build build:babel:watch",
    "build:babel:prod": "babel -D -s --minified -d build src",
    "build:babel": "babel -D -s -d build src",
    "build:babel:watch": "babel -D -s -w -d build src",
    "clean": "run-s clean:build",
    "clean:build": "rimraf build/*",
    "clean:node_modules": "rimraf node_modules",
    "clean:all": "run-p clean:build clean:node_modules",
    "test": "echo \"Error: no test specified\" && exit 1",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhisekp/Practice-Modern-JavaScript.git"
  },
  "keywords": [
    "practice",
    "modern",
    "javascript",
    "es6",
    "es7",
    "es2015",
    "es2016"
  ],
  "author": "Abhisek Pattnaik <abhisekp@engineer.com> (http://about.me/abhisekp)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abhisekp/Practice-Modern-JavaScript/issues"
  },
  "files": [
    "build"
  ],
  "homepage": "https://github.com/abhisekp/Practice-Modern-JavaScript#readme"
}
